{
  "$schema": "http://json-schema.org/draft-03/schema",
  "required": true,
  "type": "object",
  "properties": {
    "results": {
      "required": false,
      "type": "array",
      "items": {
        "required": false,
        "type": "object",
        "properties": {
          "member": {
            "required": false,
            "type": "object",
            "properties": {
              "id": {
                "required": false,
                "type": "integer"
              },
              "name": {
                "required": false,
                "type": "string"
              },
              "photo": {
                "required": false,
                "type": "object",
                "properties": {
                  "thumb": {
                    "required": false,
                    "type": "integer"
                  }
                }
              }
            }
          },
          "rsvp": {
            "required": false,
            "type": "object",
            "properties": {
              "guests": {
                "required": false,
                "type": "string"
              },
              "response": {
                "required": false,
                "type": "string"
              }
            }
          },
          "status": {
            "required": false,
            "type": "string"
          }
        }
      }
    }
  }
}
