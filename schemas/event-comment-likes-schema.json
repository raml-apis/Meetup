{
  "$schema": "http://json-schema.org/draft-03/schema",
  "required": true,
  "type": "object",
  "properties": {
    "results": {
      "required": false,
      "type": "array",
      "items": {
        "required": false,
        "type": "object",
        "properties": {
          "created": {
            "required": false,
            "type": "integer"
          },
          "event_comment_id": {
            "required": false,
            "type": "integer"
          },
          "member": {
            "required": false,
            "type": "object",
            "properties": {
              "member_city": {
                "required": false,
                "type": "string"
              },
              "member_country": {
                "required": false,
                "type": "string"
              },
              "member_id": {
                "required": false,
                "type": "integer"
              },
              "member_state": {
                "required": false,
                "type": "string"
              },
              "name": {
                "required": false,
                "type": "string"
              },
              "photo": {
                "required": false,
                "type": "object",
                "properties": {
                  "highres_link": {
                    "required": false,
                    "type": "string"
                  },
                  "photo_id": {
                    "required": false,
                    "type": "integer"
                  },
                  "photo_link": {
                    "required": false,
                    "type": "string"
                  },
                  "thumb_link": {
                    "required": false,
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
