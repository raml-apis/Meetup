queryParameters: 
  group_id: 
    description: Return <<name>> in groups with these ID numbers [separated by commas]
  group_urlname: 
    description: Return <<name>> for the group with this custom URL path
  topic: 
    description: |
      {topic, groupnum} - Return <<name>> for the group with given topic and number
  groupnum: 
    description: |
      {topic, groupnum} - Return <<name>> for the group with given topic and number
